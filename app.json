{
  "expo": {
    "name": "Project1Type",
    "slug": "Project1Type",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./public/assets/splash.png",
      "resizeMode": "cover"
    },
    // "plugins": ["expo-apple-authentication"],
    "updates": {
